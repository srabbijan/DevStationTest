<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.DevStationTest" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <item name="android:forceDarkAllowed" tools:targetApi="q">false</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryVariant">@color/color_white</item>
        <item name="colorOnPrimary">@color/colorPrimary</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/TextColorPrimaryDark</item>
        <item name="colorSecondaryVariant">@color/color_black</item>
        <item name="colorOnSecondary">@color/TextColorPrimaryDark</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" >?attr/colorPrimaryVariant</item>
        <item name="android:windowLightStatusBar">true</item>

    </style>
</resources>